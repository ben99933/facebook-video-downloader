{
  "name": "facebook-video-downloader",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build-only": "vite build",
    "dev": "vite  && npx tailwindcss -i ./src/assets/css/style.css -o ./src/assets/css/tailwind.css --minify",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "hotfix": "npx tailwindcss -i ./src/assets/css/style.css -o ./src/assets/css/tailwind.css --minify --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "autoprefixer": "^10",
    "markdown-it": "^14.1.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-playground": "^0.0.5",
    "pinia": "^2.1.7",
    "postcss": "^8",
    "tailwindcss": "^3",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.2",
    "vue-cli-plugin-tailwind": "~3.0.0",
    "vue-tsc": "^2.2.8"
  },
  "type": "commonjs"
}
